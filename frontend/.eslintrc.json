{
    "extends": ["next/core-web-vitals", "next/typescript"],
    "rules": {
        "import/newline-after-import": [
            "error",
            {
                "count": 1
            }
        ],
        "import/order": [
            "error",
            {
                "alphabetize": {
                    "order": "asc",
                    "caseInsensitive": true
                },
                "newlines-between": "always"
            }
        ],
        "sort-imports": [
            "error",
            {
                "ignoreCase": true,
                "ignoreDeclarationSort": true
            }
        ]
    }
}
